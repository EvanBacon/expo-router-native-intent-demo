{
  "expo": {
    "name": "Native Intent",
    "slug": "jun8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://github.com/expo.png",
    "scheme": "native-intent",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "appleTeamId": "QQ57RJ5UTD",
      "supportsTablet": true,
      "bundleIdentifier": "com.bacon.jun8"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["./withImageImport.js", "expo-font", "expo-router"]
  }
}
